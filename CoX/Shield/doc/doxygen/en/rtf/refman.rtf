//param None\par
\par
//This function configure the SPI as master and slave mode at the same time\par
//transfer and receive data\par
\par
//return None\par
\par
//*****************************************************************************\par
void SpiMasterSlaveModeConfig(void)\par
\{\par
   \par
   //\par
   // Configure Some GPIO pins as SPI Mode\par
   //\par
   GPIOPinFunctionSet(GPIO_FUNCTION_SPI, GPIO_PORTC_BASE,GPIO_PIN_2);\par
   GPIOPinFunctionSet(GPIO_FUNCTION_SPI, GPIO_PORTC_BASE,GPIO_PIN_3);\par
   GPIOPinFunctionSet(GPIO_FUNCTION_SPI, GPIO_PORTC_BASE,GPIO_PIN_0);\par
   GPIOPinFunctionSet(GPIO_FUNCTION_SPI, GPIO_PORTC_BASE,GPIO_PIN_1);\par
   GPIOPinFunctionSet(GPIO_FUNCTION_SPI, GPIO_PORTC_BASE,GPIO_PIN_9);\par
   GPIOPinFunctionSet(GPIO_FUNCTION_SPI, GPIO_PORTC_BASE,GPIO_PIN_8);\par
   GPIOPinFunctionSet(GPIO_FUNCTION_SPI, GPIO_PORTC_BASE,GPIO_PIN_10);\par
   GPIOPinFunctionSet(GPIO_FUNCTION_SPI, GPIO_PORTC_BASE,GPIO_PIN_11);\par
   //\par
   // Master Mode  polarity 0,Rx latched at rising edge Tx changed at rising edge\par
   // 2000000Hz 8Bits Data windth  SPI MSB First\par
   //\par
SPIConfig(SPI0_BASE,2000000,SPI_FORMAT_MODE_5|SPI_DATA_WIDTH8|SPI_MSB_FIRST|SPI_MODE_MASTER);\par
   //\par
   // Slave Mode  polarity 0,Rx latched at rising edge Tx changed at rising edge\par
   // 2000000Hz 8Bits Data windth  SPI MSB First\par
   //\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(SPI1_BASE + SPI_CNTRL) = 0x00000000;\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(SPI1_BASE + SPI_CNTRL) |= 0x00040844;\par
   //\par
   // Software Control Transfer Select SS0 and SS1\par
   //\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABTP" }{}}{\fldrslt {\cs37\ul\cf2 xSPISSSet}}}
(SPI0_BASE,{\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABSS" }{}}{\fldrslt {\cs37\ul\cf2 xSPI_SS_SOFTWARE}}}
,SPI_SS0);   \par
\par
   //\par
   // Level Trig Slave at High Level\par
   //\par
   SPISSConfig(SPI0_BASE, SPI_SS_LEVEL_TRIGGER,SPI_SS_ACTIVE_HIGH_RISING);\par
   SPISSConfig(SPI1_BASE, SPI_SS_LEVEL_TRIGGER,SPI_SS_ACTIVE_HIGH_RISING);\par
\par
   for(i=0;i<1;i++)\par
   \{\par
       SPIDataWrite(SPI0_BASE,SendData,15);\par
       SPIDataRead(SPI1_BASE,ReceData, 16);\par
       TestIOPut('O');\par
       TestIOPut('K');\par
   \}\par
   TestIOPut('\\n');\par
   for(i=0;i<18;i++)\par
   \{\par
       TestIOPut(ReceData[i]);  \par
   \}\par
\}\par
//******************************************************************************\par
//\par
// The main function of the SPI transfer  \par
//\par
//******************************************************************************\par
int main(void)\par
\{\par
   SpiReceiveTransferInit();\par
   SpiMasterSlaveModeConfig();\par
   while(1)\par
   \{    \par
   \}\par
       \par
\}\par
}
