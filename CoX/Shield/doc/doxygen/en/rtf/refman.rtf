{\tc\tcl2 \v 3. Usage & Program Examples}
The following example shows how to use the RTC APIs to initialize the RTC, and to write and read time from RTC registers.\par
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid tTime tTime1, tTime2;\par
unsigned long ulj;\par
unsigned long ulTimeAlarm[2] = \{RTC_TIME_CURRENT, RTC_TIME_ALARM\};\par
unsigned long ulMode[8] = \{RTC_TIME_TICK_1, RTC_TIME_TICK_1_2, RTC_TIME_TICK_1_4,\par
                           RTC_TIME_TICK_1_8, RTC_TIME_TICK_1_16, RTC_TIME_TICK_1_32,\par
                           RTC_TIME_TICK_1_64, RTC_TIME_TICK_1_128\};\par
\par
unsigned long xRTCCallback(void *pvCBData, \par
                                       unsigned long ulEvent,\par
                                       unsigned long ulMsgParam,\par
                                       void *pvMsgData)\par
\{\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAADR" }{}}{\fldrslt {\cs37\ul\cf2 xIntMasterDisable}}}
();\par
    ulj++;\par
    return 0;\par
\}\par
static void xrtc001Int()\par
\{\par
    RTCTimeInit();\par
    \par
    tTime2.ulSecond = 40;     \par
    \par
    tTime2.ulMinute = 20; \par
    \par
    tTime2.ulHour = 17; \par
    \par
    tTime2.ulMDay = 11;  \par
    \par
    tTime2.ulMonth = 8;   \par
    \par
    tTime2.ulYear = 2011;    \par
    \par
    tTime2.ulWDay = 3;   \par
\par
    tTime2.ulFormat = RTC_TIME_24H;\par
    \par
    //\par
    //write current time to corresponding register.\par
    //\par
    RTCTimeWrite(&tTime2, ulTimeAlarm[0]);\par
    \par
    RTCWriteEnable();\par
    \par
    //\par
    //alarm interrupt after 20 seconds.\par
    //\par
    tTime2.ulSecond +=10;\par
    \par
    //\par
    //write current time to corresponding register.\par
    //\par
    RTCTimeWrite(&tTime2, ulTimeAlarm[1]);\par
    \par
    RTCWriteEnable();\par
    \par
    RTCIntCallbackInit(xRTCCallback);\par
    \par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAADU" }{}}{\fldrslt {\cs37\ul\cf2 xIntEnable}}}
(INT_RTC);\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAADQ" }{}}{\fldrslt {\cs37\ul\cf2 xIntMasterEnable}}}
();\par
    //\par
    //enable alarm interrupt.\par
    //\par
    RTCIntEnable(RTC_INT_ALARM);\par
    \par
    TestAssert(RTC_INT_ALARM == ( {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(RTC_RIER) & 0x00000003),\par
                  "xrtc API \\"RTCIntEnable\\" error!");\par
   \par
    while(ulj !=1);\par
    \par
    //\par
    //disable alarm interrupt.\par
    //\par
    RTCIntDisable(RTC_INT_ALARM);\par
   \par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAADQ" }{}}{\fldrslt {\cs37\ul\cf2 xIntMasterEnable}}}
();\par
    //\par
    //set second interrupt.\par
    //\par
    RTCTickModeSet(ulMode[0]);\par
   \par
    //\par
    //enable tick interrupt.\par
    //\par
    RTCIntEnable(RTC_INT_TIME_TICK);\par
    \par
    TestAssert(RTC_INT_TIME_TICK == ( {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(RTC_RIER) & 0x00000003),\par
                  "xrtc API \\"RTCIntEnable\\" error!");\par
 \par
    while(ulj !=2);\par
    \par
    TestAssert(1 == 1,\par
                  "xrtc API \\"RTCIntCallbackInit\\" error!");\par
\}\par
}
