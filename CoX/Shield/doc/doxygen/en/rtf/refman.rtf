//*****************************************************************************\par
//\par
// Systick Interrupt serive function\par
//\par
// When the interupt happen ,print some words In the super terminal through \par
// uart0.\par
//\par
// return none\par
//\par
//*****************************************************************************\par
void IntSystickHandler(void)\par
\{\par
    //\par
    //Clear the systick pending bit  \par
    //\par
   //  xIntPendClear(FAULT_SYSTICK);\par
    \par
   //\par
   // Print a "hello systick" through uart0\par
   //\par
   Print("wake up the wfi first/n");\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAEB" }{}}{\fldrslt {\cs37\ul\cf2 xSysTickDisable}}}
();\par
   \par
\}\par
\par
//*****************************************************************************\par
//\par
// The main function of the systick  \par
//\par
//*****************************************************************************\par
int main(void)\par
\{\par
   //\par
   //Set the external 8MHZ clock as system clock \par
   //\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABZD" }{}}{\fldrslt {\cs37\ul\cf2 xSysCtlClockSet}}}
(8000000, SysCtl_USE_OSC | SysCtl_OSC_MAIN | SysCtl_XTAL_8MHZ);\par
   \par
   //\par
   //UART initialize\par
   //\par
   UART0Configuration();\par
   Print("uart0 is ok, wait for the wfi/n");\par
   //\par
   //Enable the systick interrupt\par
   //\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAEC" }{}}{\fldrslt {\cs37\ul\cf2 xSysTickIntEnable}}}
();\par
   \par
   //\par
   //Set the systick as 1 time per second\par
   //\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAEE" }{}}{\fldrslt {\cs37\ul\cf2 xSysTickPeriodSet}}}
(8000000);\par
   \par
   //\par
   //Enable the systick\par
   //\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAEA" }{}}{\fldrslt {\cs37\ul\cf2 xSysTickEnable}}}
();\par
   \par
   //\par
   // Enter the sleep mode\par
   //\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAER" }{}}{\fldrslt {\cs37\ul\cf2 xCPUwfi}}}
();\par
   \par
   //\par
   // go on excute Print code words after wake up. \par
   //\par
   Print("the go on print/n");\par
   while(1);\par
   \par
   \par
\}\par
