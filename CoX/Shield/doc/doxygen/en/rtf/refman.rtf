\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid void uart_test()\par
\{\par
    char ch[] = "Hello, UART!";    \par
\par
    SysCtlKeyAddrUnlock();\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(SYSCLK_PWRCON) |= SYSCLK_PWRCON_XTL12M_EN;\par
\par
    SysCtlDelay(10000);\par
\par
    XPinTypeUART(U0RX,{\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAOB" }{}}{\fldrslt {\cs37\ul\cf2 PB0}}}
);\par
    XPinTypeUART(U0TX,{\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAOC" }{}}{\fldrslt {\cs37\ul\cf2 PB1}}}
);\par
\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABYW" }{}}{\fldrslt {\cs37\ul\cf2 xSysCtlPeripheralReset}}}
({\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABUN" }{}}{\fldrslt {\cs37\ul\cf2 xSYSCTL_PERIPH_UART0}}}
);\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABYX" }{}}{\fldrslt {\cs37\ul\cf2 xSysCtlPeripheralEnable}}}
({\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABUN" }{}}{\fldrslt {\cs37\ul\cf2 xSYSCTL_PERIPH_UART0}}}
);\par
    SysCtlPeripheralClockSourceSet(SYSCTL_PERIPH_UART_S_EXT12M);\par
\par
    UARTConfigSetExpClk(UART0_BASE, 115200, (UART_CONFIG_WLEN_8 | UART_CONFIG_STOP_ONE | UART_CONFIG_PAR_NONE));\par
\par
    UARTEnable(UART0_BASE, (UART_BLOCK_UART | UART_BLOCK_TX | UART_BLOCK_RX));\par
\par
    UARTBufferWrite(UART0_BASE, ch, sizeof(ch)/sizeof(*ch));\par
             \par
    while(1);\par
\}\par
}
