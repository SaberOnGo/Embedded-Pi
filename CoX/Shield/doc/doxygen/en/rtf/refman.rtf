//*****************************************************************************\par
//\par
// \\biref The main example function\par
//\par
// \\return  none\par
//\par
//*****************************************************************************\par
void OneSecondClock(void)\par
\{\par
    //\par
    //Set the external 12MHZ clock as system clock \par
    //\par
        SysCtlKeyAddrUnlock();\par
        {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(SYSCLK_PWRCON) |= SYSCLK_PWRCON_XTL12M_EN;\par
    \par
    //\par
    // Set the timer clock\par
    //\par
    SysCtlPeripheralClockSourceSet(SYSCTL_PERIPH_TMR0_S_EXT12M);\par
    \par
    UART0Configuration();\par
    \par
    // \par
    // Enable tiemr0\par
    // \par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABYX" }{}}{\fldrslt {\cs37\ul\cf2 xSysCtlPeripheralEnable}}}
({\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABUJ" }{}}{\fldrslt {\cs37\ul\cf2 xSYSCTL_PERIPH_TIMER0}}}
);  \par
    \par
    //\par
    // Clear the flag first\par
    //\par
    xTimerIntClear(TIMER0_BASE, 0, TIMER_INT_MATCH);\par
    while(xTimerIntStatus(TIMER0_BASE, 0, TIMER_INT_MATCH));\par
    \par
    // \par
    // Config as One shot mode\par
    //        \par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACAF" }{}}{\fldrslt {\cs37\ul\cf2 xTimerInitConfig}}}
(TIMER0_BASE, 0, TIMER_MODE_PERIODIC, 1000);\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACAL" }{}}{\fldrslt {\cs37\ul\cf2 xTimerPrescaleSet}}}
(TIMER0_BASE, 0, 91);\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACAQ" }{}}{\fldrslt {\cs37\ul\cf2 xTimerMatchSet}}}
(TIMER0_BASE, 0, 0x1ffff);\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACAT" }{}}{\fldrslt {\cs37\ul\cf2 xTimerIntEnable}}}
(TIMER0_BASE, 0, TIMER_INT_MATCH);\par
    \par
    //\par
    // Start the timer\par
    //\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAACAG" }{}}{\fldrslt {\cs37\ul\cf2 xTimerStart}}}
(TIMER0_BASE, 0);\par
        \par
    //\par
    // One shot mode test.\par
    //\par
    while(1)\par
    \{       \par
        //\par
        // wait until the timer data register reach equel to compare register\par
        //\par
        while(!xTimerIntStatus(TIMER0_BASE, 0, TIMER_INT_MATCH));\par
        Print("one second has gone...");\par
        Print("\\r\\n");\par
        xTimerIntClear(TIMER0_BASE, 0, TIMER_INT_MATCH);\par
    \}  \par
    \par
\par
\}\par
\par
//*****************************************************************************\par
//\par
// \\biref uart0 configre \par
//\par
// \\return  none\par
//\par
//*****************************************************************************\par
void UART0Configuration(void)\par
\{\par
  \par
        {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAUU" }{}}{\fldrslt {\cs37\ul\cf2 xSPinTypeUART}}}
(U0RX,{\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAOB" }{}}{\fldrslt {\cs37\ul\cf2 PB0}}}
);\par
        {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAUU" }{}}{\fldrslt {\cs37\ul\cf2 xSPinTypeUART}}}
(U0TX,{\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAOC" }{}}{\fldrslt {\cs37\ul\cf2 PB1}}}
);\par
\par
        //xSysCtlPeripheralReset(xSYSCTL_PERIPH_UART0);\par
        {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABYX" }{}}{\fldrslt {\cs37\ul\cf2 xSysCtlPeripheralEnable}}}
({\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABUN" }{}}{\fldrslt {\cs37\ul\cf2 xSYSCTL_PERIPH_UART0}}}
);\par
    SysCtlPeripheralClockSourceSet(SYSCTL_PERIPH_UART_S_EXT12M);\par
\par
        {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(UART0_BASE + UART_FCR) |= 0x6;\par
        {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(UART0_BASE + UART_FCR) &= ~(0xF0);\par
        {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(UART0_BASE + UART_FCR) |= 0;\par
\par
        {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(UART0_BASE + UART_LCR) = 3;\par
        {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(UART0_BASE + UART_BAUD) = 0x3F000066;\par
\par
        SysCtlDelay(10000);\par
\}\par
\par
//*****************************************************************************\par
//\par
// \\breif uart send function\par
//\par
// \\return none\par
//\par
//*****************************************************************************\par
void Print(char *pcMsg)\par
\{\par
    unsigned char ch;\par
    while (*pcMsg != '\\0')\par
    \{\par
        //\par
        // Put a character in the output buffer.\par
        //\par
        ch = *pcMsg++;\par
\par
        while(({\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(UART0_BASE + UART_FSR) & (0x400000))!=0x400000);\par
            //\par
           // Write this character to the transmit FIFO.\par
            //\par
            {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(UART0_BASE + UART_THR) = ch;\par
    \}\par
\}\par
}
