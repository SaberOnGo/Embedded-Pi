//*****************************************************************************\par
void SpiReceiveTransferInit(void)\par
\{\par
   SysCtlKeyAddrUnlock();\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(SYSCLK_PWRCON) |= SYSCLK_PWRCON_XTL12M_EN;\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(SYSCLK_PLLCON) &= ~SYSCLK_PLLCON_OE;\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(SYSCLK_PLLCON) &= ~SYSCLK_PLLCON_PD;\par
   //\par
   // Set SysClk 36MHz using Extern 12M oscillator\par
   //\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABZD" }{}}{\fldrslt {\cs37\ul\cf2 xSysCtlClockSet}}}
(36000000, {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABUR" }{}}{\fldrslt {\cs37\ul\cf2 xSYSCTL_OSC_MAIN}}}
|{\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABVD" }{}}{\fldrslt {\cs37\ul\cf2 xSYSCTL_XTAL_12MHZ}}}
);\par
   //\par
   // Enable Peripheral SPI0,SPI1\par
   //\par
   SysCtlPeripheralEnable(SYSCTL_PERIPH_SPI0);\par
   SysCtlPeripheralEnable(SYSCTL_PERIPH_SPI1);\par
   SPIIntEnable(SPI0_BASE, TransferOver);\par
