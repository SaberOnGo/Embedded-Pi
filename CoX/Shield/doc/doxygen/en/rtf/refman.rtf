void UART0Configuration(void)\par
\{\par
   //\par
   //Enable the uart0 peripheral\par
   //\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABYX" }{}}{\fldrslt {\cs37\ul\cf2 xSysCtlPeripheralEnable}}}
(SysCtl_PERIPH_GPIOA);\par
   {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABYX" }{}}{\fldrslt {\cs37\ul\cf2 xSysCtlPeripheralEnable}}}
(SysCtl_PERIPH_UART0);\par
   \par
   //\par
   // Set GPIO A0 and A1 as UART pins.\par
   //\par
   xGPIOPinTypeUART(GPIO_PORTA_BASE, GPIO_PIN_0 | GPIO_PIN_1);\par
\par
   //\par
   // Configure the UART for 115,200, 8-N-1 operation.\par
   //\par
   UARTConfigSetExpClk(UART0_BASE, SysCtlClockGet(),115200 ,\par
        (UART_CONFIG_WLEN_8 | UART_CONFIG_STOP_1 | UART_CONFIG_PAR_NONE),\\\par
          (UART_CTL_UARTEN | UART_CTL_TXE | UART_CTL_RXE));\par
\par
   UARTEnable(UART0_BASE, UART_CTL_UARTEN | UART_CTL_TXE | UART_CTL_RXE);\par
\}\par
\par
//*****************************************************************************\par
//\par
// uart send function\par
//\par
// return none\par
//\par
//*****************************************************************************\par
void Print(char *pcMsg)\par
\{\par
    unsigned char ch;\par
    while (*pcMsg != '\\0')\par
    \{\par
       //\par
       // Put a character in the output buffer.\par
       //\par
       ch = *pcMsg++;\par
       while(({\field {\*\fldinst { HYPERLINK  \\l "AAAAAAABMA" }{}}{\fldrslt {\cs37\ul\cf2 xHWREG}}}
(UART0_BASE + UART_O_FR) & UART_FR_TXFF));\par
       UARTCharPutNonBlocking(UART0_BASE, ch);\par
    \}\par
\}\par
}
